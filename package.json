    "task",
    "queue",
    "async",
    "await",
    "promises",
    "bluebird"
  ],
  "author": "islizeqiang",
  "license": "MIT",
  "homepage": "https://github.com/islizeqiang/p-limiter#readme",
  "bugs": {
    "url": "https://github.com/islizeqiang/p-limiter/issues"
